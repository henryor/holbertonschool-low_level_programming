#include "main.h" 

/**
* _strcat - function appends the src string to the dest string
* @dest: append by src
* @src: string to append to dest
* Return: pointer to the resulting string dest

char *_strcat(char *dest, char *src);
{
int i,j;

i=j=0;
while(*(dest + i))
i++
while((*dest + i) = *(src + j)))
{
i++;
j++;
}
return (dest);
}